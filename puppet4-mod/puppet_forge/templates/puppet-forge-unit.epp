[Unit]
Description=Puppet forge server
After=network.target

[Service]
User=puppetforge
Group=puppetforge
PIDFile=/var/lib/puppetforge/puppetforge.pid
ExecStart=/usr/local/bin/run-puppet-forge
Restart=on-failure

[Install]
WantedBy=multi-user.target


