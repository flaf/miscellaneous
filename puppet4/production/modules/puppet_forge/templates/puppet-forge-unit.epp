<%- |$puppet_forge_pid| -%>
### This file is managed by Puppet, don't edit it ###

[Unit]
Description=Puppet forge server
After=network.target

[Service]
User=puppetforge
Group=puppetforge
PIDFile=<%= $puppet_forge_pid %>
ExecStart=/usr/local/bin/puppet-forge
Restart=on-failure

[Install]
WantedBy=multi-user.target


